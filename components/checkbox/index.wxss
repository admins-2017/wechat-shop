.container {
  width: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

.bg-checked{
  background-color: #157658;
}

.bg-unchecked{
  background-color: #DCEBE6;
  border: 2rpx solid #157658;
}

.sleeve{
  font-family: sleeve !important;
  color: #fff !important;
}
