.container{
    width: 100%;
    height: 320rpx;
    box-sizing: border-box;
    padding: 10rpx;
    background-color: #fff;
}

.inner-container{
    width: 100%;
    height: 100%;
    border: 1rpx dashed #DCEBE6;
}

.item{
    padding: 10rpx !important;
}

.grid-item{
    width: 100%;
    padding: 0rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.img{
    width: 60rpx;
    height: 60rpx;
}

.text{
    padding-top: 10rpx;
    font-size: 24rpx;
    color: #333333;
}